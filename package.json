{
  "name": "matching-game",
  "version": "1.0.0",
  "description": "*yadda yadda yadda*",
  "source": "index.html",
  "scripts": {
    "clean-dist": "rm -rf dist && mkdir dist",
    "copy-html": "cp index.html dist",
    "copy-js": "mkdir dist/js && cp ./js/* dist/js",
    "copy-css": "mkdir dist/css && cp ./css/* dist/css",
    "copy-sounds": "mkdir dist/ogg && mkdir dist/wav && cp -r ./ogg/* dist/ogg && cp -r ./wav/* dist/wav",
    "copy-files": "npm run copy-html & npm run copy-js & npm run copy-css & npm run copy-sounds",
    "init": "npm run clean-dist && npm run copy-files",
    "start": "npm run init && parcel --open",
    "build": "npm run init && parcel build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rockleejb/matching-game.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/rockleejb/matching-game/issues"
  },
  "homepage": "https://github.com/rockleejb/matching-game#readme",
  "devDependencies": {
    "parcel": "^2.12.0"
  },
  "dependencies": {
    "bootstrap": "^5.2.3"
  }
}
